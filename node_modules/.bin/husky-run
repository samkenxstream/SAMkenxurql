#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/SAMkenxurql/SAMkenxurql/node_modules/.pnpm/husky-v4@4.3.8/node_modules/husky-v4/bin/node_modules:/home/runner/work/SAMkenxurql/SAMkenxurql/node_modules/.pnpm/husky-v4@4.3.8/node_modules/husky-v4/node_modules:/home/runner/work/SAMkenxurql/SAMkenxurql/node_modules/.pnpm/husky-v4@4.3.8/node_modules:/home/runner/work/SAMkenxurql/SAMkenxurql/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/SAMkenxurql/SAMkenxurql/node_modules/.pnpm/husky-v4@4.3.8/node_modules/husky-v4/bin/node_modules:/home/runner/work/SAMkenxurql/SAMkenxurql/node_modules/.pnpm/husky-v4@4.3.8/node_modules/husky-v4/node_modules:/home/runner/work/SAMkenxurql/SAMkenxurql/node_modules/.pnpm/husky-v4@4.3.8/node_modules:/home/runner/work/SAMkenxurql/SAMkenxurql/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../husky-v4/bin/run.js" "$@"
else
  exec node  "$basedir/../husky-v4/bin/run.js" "$@"
fi
