#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/SAMkenxurql/SAMkenxurql/node_modules/.pnpm/surge@0.21.7/node_modules/surge/lib/node_modules:/home/runner/work/SAMkenxurql/SAMkenxurql/node_modules/.pnpm/surge@0.21.7/node_modules/surge/node_modules:/home/runner/work/SAMkenxurql/SAMkenxurql/node_modules/.pnpm/surge@0.21.7/node_modules:/home/runner/work/SAMkenxurql/SAMkenxurql/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/SAMkenxurql/SAMkenxurql/node_modules/.pnpm/surge@0.21.7/node_modules/surge/lib/node_modules:/home/runner/work/SAMkenxurql/SAMkenxurql/node_modules/.pnpm/surge@0.21.7/node_modules/surge/node_modules:/home/runner/work/SAMkenxurql/SAMkenxurql/node_modules/.pnpm/surge@0.21.7/node_modules:/home/runner/work/SAMkenxurql/SAMkenxurql/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/surge@0.21.7/node_modules/surge/lib/cli.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/surge@0.21.7/node_modules/surge/lib/cli.js" "$@"
fi
